{
  "description": "Training a 3 layers Categorical VAE with linear connections.",
  "args": [
    "--estimator ww",
    "--estimator tvo-S20",
    "--estimator copt",
    "--estimator copt-autoalpha"
  ],
  "global": "--silent --epochs 10000 --lr_reduce_steps 0",
  "parameters": {
    "model":["hierarchical"],
    "prior": ["normal"],
    "learn_prior" : [true],
    "N": [16],
    "K": [16],
    "freebits": [0],
    "norm": ["layernorm","none"],
    "nlayers" :[1, 2, 3],
    "dropout" : [0, 0.2],
    "depth" : [3],
    "hdim" : [100],
    "kdim" : [0],
    "iw" : [64],
    "seed":[47, 45, 32],
    "dataset" : ["binmnist"],
    "mini": [true],
    "only_train_set": [true],
    "grad_samples" : [10],
    "iw_valid" : [1000],
    "iw_test" : [5000],
    "eval_freq": [100],
    "bs": [64],
    "lr": [1e-3],
    "valid_bs": [2],
    "test_bs": [2],
    "grad_bs": [20]
  }
}