{
  "description": "Training a 3 layers Sigmoid Belief Network with linear connections.",
  "args": [
    "--estimator copt-alpha0 --gamma_min 1e-2 --warmup 1000000 --warmup_mode log",
    "--estimator copt-alpha1 --gamma_min 1e-2 --warmup 1000000 --warmup_mode log"
  ],
  "global": "--silent --deterministic --nsteps 4000000",
  "parameters": {
    "id": ["alphas-wrp"],
