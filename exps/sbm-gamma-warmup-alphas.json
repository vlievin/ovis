    "dataset" : ["binmnist"],
    "lr": [3e-4],
    "lr_reduce_steps":[0],
    "bs": [24],
    "valid_bs": [10],
    "test_bs": [10],
    "grad_bs": [24],
    "grad_samples" : [200],
    "iw_test" : [5000],
    "iw_valid" : [5000],
    "eval_freq": [30],
    "max_eval": [10000],
    "grad_key" : ["inference_network"],
    "workers": [2],
    "mc_au_analysis": [1000],
    "npoints_au_analysis": [1000]
  }
