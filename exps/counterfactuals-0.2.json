{
  "description": "Comparing CovBaseline vs. Vimco based on the variance of the gradients using counterfactual evaluation usibng mini binary datasets.",
  "args": [
    "--estimator vimco-arithmetic",
    "--estimator covbaseline-arithmetic"
  ],
  "global": "--silent --deterministic --epochs 100 --lr_reduce_steps 3 --model vae --bs 32 --hdim 256 --iw_valid 4 --N 16 --counterfactuals=vimco-arithmetic,covbaseline-arithmetic",
  "parameters": {
    "iw" : [3, 5, 10, 20, 50],
    "seed":[13, 64, 47, 45, 32],
    "dataset" : ["binmnist", "omniglot"]
  }
}