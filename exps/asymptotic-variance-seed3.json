{
  "description": "Analysing the asymptotic variance as in `Tighter Variational Bounds are not necessarily Better`",
  "script": "asymptotic_variance.py",
  "args": [
    "--estimator copt-aux50",
    "--estimator vimco-geometric",
    "--estimator copt-aux10",
    "--estimator copt-aux1",
    "--estimator pathwise-iwae",
    "--estimator vimco-arithmetic",
    "--estimator copt-alpha1"
  ],
  "global": "--silent --deterministic",
  "parameters": {
    "mc_samples": [1e4],
    "samples_per_batch": [5000],
    "iw_steps": [5],
    "iw_min": [3],
    "iw_max": [1e3],
    "npoints" : [1024],
    "D" : [20],
    "seed" : [3],
    "oracle": ["copt"],
    "iw_oracle" : [1000],
    "mc_oracle": [100]
  }
}
