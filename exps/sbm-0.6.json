{
  "description": "Training a 3 layers Sigmoid Belief Network with linear connections.",
  "args": [
    "--estimator vimco",
    "--estimator copt",
    "--estimator copt-uniform",
    "--estimator copt-ess",
    "--estimator vimco-alpha0",
    "--estimator tvo-part21",
    "--estimator wake-wake",
    "--estimator reinforce-baseline"
  ],
  "global": "--silent --deterministic --nsteps 1000000 --lr_reduce_steps 0",
  "parameters": {
    "model":["hierarchical"],
    "prior": ["bernoulli"],
    "N": [200],
    "nlayers" :[0],
    "depth" : [3],
    "iw" : [100, 50, 10, 5],
    "seed":[47, 45, 32],
    "dataset" : ["binmnist"],
    "only_train_set": [true],
    "grad_samples" : [1000],
    "iw_valid" : [1000],
    "iw_test" : [5000],
    "eval_freq": [10],
    "bs": [64],
    "lr": [3e-4],
    "valid_bs": [10],
    "test_bs": [5],
    "grad_bs": [20]
  }
}
