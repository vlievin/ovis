{
  "description": "Training a 1 layer VAE with non-linnear connections and Gaussian prior.",
  "args": [
    "--estimator copt-aux10"
  ],
  "global": "--silent --nsteps 4000000",
  "parameters": {
    "model":["hierarchical"],
    "prior": ["normal"],
    "norm": ["none"],
    "learn_prior" : [true],
    "N": [200],
    "hdim" : [200],
    "nlayers" :[2],
    "depth" : [1],
    "gamma_min" : [5e-2, 1e-2, 5e-3, 1e-3],
    "warmup_mode" : ["log"],
    "warmup" :[1000000, 500000],
    "iw" : [50],
    "seed":[1, 2, 3],
    "dataset" : ["binmnist"],
    "lr": [3e-4],
    "lr_reduce_steps":[0],
    "bs": [64],
    "valid_bs": [10],
    "test_bs": [10],
    "grad_bs": [24],
    "grad_samples" : [200],
    "iw_test" : [1000],
    "iw_valid" : [1000],
    "eval_freq": [50],
    "max_eval": [10000],
    "grad_key" : ["inference_network"],
    "workers": [2],
    "mc_au_analysis": [1000],
    "npoints_au_analysis": [1000]
  }
}