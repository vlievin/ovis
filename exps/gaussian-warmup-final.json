{
  "description": "Training a 1 layers Gaussian VAE with non-linear connections.",
  "args": [
    "--estimator pathwise-iwae",
    "--estimator copt-alpha0",
    "--estimator tvo-S5-config2",
    "--estimator copt-aux1",
    "--estimator copt-aux10",
    "--estimator ww",
    "--estimator vimco",
    "--estimator copt"
  ],
  "global": "--silent --deterministic --nsteps 4000000",
  "parameters": {
    "model":["hierarchical"],
    "prior": ["normal"],
    "learn_prior" : [true],
    "norm": ["none"],
    "N": [200],
    "hdim": [200],
    "nlayers" :[2],
    "depth" : [1],
    "iw" : [5, 10, 50],
    "seed":[1, 2, 3],
    "dataset" : ["binmnist"],
    "warmup": [100000],
    "beta_min" : [1.0],
    "warmup_estimator" : ["tvo-S5-config2"],
    "lr": [1e-3],
    "lr_reduce_steps":[0],
    "bs": [24],
    "valid_bs": [10],
    "test_bs": [10],
    "grad_bs": [24],
    "grad_samples" : [500],
    "iw_test" : [5000],
    "iw_valid" : [5000],
    "eval_freq": [30],
    "max_eval": [10000],
    "grad_key" : ["inference_network"],
    "workers": [2],
    "mc_au_analysis": [1000],
    "npoints_au_analysis": [1000]
  }
}