{
  "description": "Benchmarking models on the Gaussian Mixture Model from `Revisiting Reweighted Wake-Sleep",
  "args": [
    "--estimator ovis-S1",
    "--estimator ovis-S10",
    "--estimator ovis-S50",
    "--estimator ovis-gamma1",
    "--estimator ovis-gamma0",
    "--estimator vimco-arithmetic",
    "--estimator vimco-geometric",
    "--estimator tvo-part5-beta2",
    "--estimator wake-wake",
    "--estimator reinforce"
  ],
  "global": "--silent --deterministic --nsteps 100000 --lr_reduce_steps 0",
  "parameters": {
    "N": [20],
    "hdim": [16],
    "iw" : [20, 10, 5, 2],
    "seed": [11, 11, 12, 13, 14],
    "dataset": ["gmm-toy"],
    "optimizer": ["adam"],
    "grad_samples" : [500],
    "grad_key": ["phi"],
    "grad_bs" : [100],
    "grad_epsilon": [1e-12],
    "bs" : [100],
    "valid_bs" : [100],
    "lr": [1e-3 ],
    "iw_test": [100],
    "iw_valid": [1000],
    "eval_freq": [5]
  }

}