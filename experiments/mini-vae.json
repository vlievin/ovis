{
  "description": "Training a 1 layer VAE with non-linnear connections and Gaussian prior on a toy dataset.",
  "args": [
    "--estimator pathwise-iwae",
    "--estimator tvo-part5-config2",
    "--estimator ovis-gamma1 --alpha 0.7"
  ],
  "global": "--silent --deterministic --nsteps 100000",
  "parameters": {
    "model":["vae"],
    "prior": ["normal"],
    "N": [32],
    "hdim" : [64],
    "nlayers" :[2],
    "iw" : [50, 10, 5],
    "seed":[1, 2],
    "dataset" : ["shapes"],
    "lr": [3e-4],
    "lr_reduce_steps":[0],
    "bs": [32],
    "valid_bs": [10],
    "test_bs": [10],
    "grad_bs": [32],
    "grad_samples" : [100],
    "iw_test" : [1000],
    "iw_valid" : [1000],
    "eval_freq": [10],
    "max_eval": [10000],
    "grad_key" : ["inference_network"],
    "workers": [1],
    "mc_au_analysis": [1000],
    "npoints_au_analysis": [1000]
  }
}