{
  "description": "Fitting the Gaussian toy model from the asymptotic analysis",
  "args": [
    "--estimator pathwise-iwae",
    "--estimator tvo-S5-beta3",
    "--estimator ovis-gamma0",
    "--estimator ovis-gamma1",
    "--estimator ovis-S1",
    "--estimator ovis-S10",
    "--estimator ovis-S50",
    "--estimator vimco-arithmetic",
    "--estimator vimco-geometric",
    "--estimator wake-wake"
  ],
  "global": "--silent --deterministic",
  "parameters": {
    "epochs": [5000],
    "lr_reduce_steps": [0],
    "lr": [1e-3],
    "optimizer": ["adam"],
    "iw" : [100, 30, 10],
    "seed":[1,2,3,4,5],
    "dataset" : ["gaussian-toy"],
    "N": [20],
    "iw_valid" : [5000],
    "iw_test" : [5000],
    "eval_freq": [50],
    "grad_samples": [1000],
    "bs": [100],
    "grad_bs" : [100],
    "valid_bs": [10],
    "test_bs": [10],
    "grad_key": ["A,b"]
  }
}
